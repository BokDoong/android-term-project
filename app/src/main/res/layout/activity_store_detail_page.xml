<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/entire_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FFFFFF">

    <RelativeLayout
        android:id="@+id/status_bar"
        android:layout_width="match_parent"
        android:layout_height="44dp">

        <TextView
            android:id="@+id/time"
            android:layout_width="54dp"
            android:layout_height="18dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="12dp"
            android:layout_centerVertical="true"
            android:text="@string/time"
            android:gravity="center_horizontal|top"
            android:textSize="15sp"
            android:letterSpacing="-0.02"
            android:textColor="#1F2021"
            />

        <ImageView
            android:id="@+id/cellular_co"
            android:layout_width="17dp"
            android:layout_height="10.67dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="61dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="17dp"
            android:src="@drawable/cellular"
            />

        <ImageView
            android:id="@+id/wifi"
            android:layout_width="15.33dp"
            android:layout_height="11dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="40.67dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="17dp"
            android:src="@drawable/wifi"
            />

        <ImageView
            android:id="@+id/battery"
            android:layout_width="24.33dp"
            android:layout_height="11.33dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="358dp"
            android:layout_marginTop="17dp"
            android:src="@drawable/battery"/>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/header_02"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:background="#FFFFFF"
        android:layout_below="@+id/status_bar">


        <ImageButton
            android:id="@+id/back_button"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:backgroundTint="#FFFFFF"
            android:src="@drawable/direction">
        </ImageButton>

        <TextView
            android:id="@+id/stored_id"
            android:layout_toRightOf="@id/back_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="0dp"
            android:layout_marginBottom="16dp"
            android:fontFamily="@font/pretendard_bold"
            android:letterSpacing="-0.02"
            android:lineHeight="20sp"
            android:text="가게 상세"
            android:textSize="20sp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/store_image"
        android:layout_width="3780dp"
        android:layout_height="253dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="-6dp"
        android:layout_marginTop="105dp"
        android:layout_marginEnd="-13dp"
        android:background="@drawable/filter"
        android:clipToOutline="true"></RelativeLayout>

    <RelativeLayout
        android:id="@+id/store_info"
        android:layout_width="match_parent"
        android:layout_height="58dp"
        android:layout_below="@+id/store_image">

        <LinearLayout
            android:layout_marginTop="15dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/name_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:fontFamily="@font/pretendard_bold"
                android:letterSpacing="-0.02"
                android:lineHeight="26sp"
                android:text="울산대학교 공차점"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/smallc_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:fontFamily="@font/pretendard"
                android:letterSpacing="0.02"
                android:lineHeight="18sp"
                android:text="카페"
                android:textColor="#B5B6BD"
                android:textSize="12sp"/>

            <ImageView
                android:id="@+id/star"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="180dp"
                android:layout_gravity="center"
                android:clipToOutline="true"
                android:src="@drawable/star" />

            <TextView
                android:id="@+id/rating_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="4dp"
                android:fontFamily="@font/pretendard"
                android:gravity="center"
                android:letterSpacing="0.01"
                android:lineHeight="20sp"
                android:text="@string/rating_id"
                android:textColor="#5C5D61"
                android:textSize="18sp"/>
        </LinearLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/frame_9640"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="416dp"
        android:layout_marginEnd="2dp" >

        <LinearLayout
            android:layout_width="343dp"
            android:layout_height="24dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/location"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:clipToOutline="true"
                android:layout_marginLeft="5dp"
                android:gravity="center"
                android:src="@drawable/location"/>

            <TextView
                android:id="@+id/address_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="울산 남구 대학로 169번길 2"
                android:lineSpacingExtra="3sp"
                android:layout_gravity="center"
                android:gravity="top"
                android:textSize="14sp"
                android:textColor="#3F3F42"
                android:fontFamily="@font/pretendard"
                android:lineHeight="20sp"
                />



        </LinearLayout>

    </RelativeLayout>



</RelativeLayout>